{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/admin/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/admin/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/admin",
  "projects": {
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "delivery": {
      "type": "application",
      "root": "apps/delivery",
      "entryFile": "main",
      "sourceRoot": "apps/delivery/src",
      "compilerOptions": {
        "tsConfigPath": "apps/delivery/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "restaurent": {
      "type": "application",
      "root": "apps/restaurent",
      "entryFile": "main",
      "sourceRoot": "apps/restaurent/src",
      "compilerOptions": {
        "tsConfigPath": "apps/restaurent/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "customers": {
      "type": "application",
      "root": "apps/customers",
      "entryFile": "main",
      "sourceRoot": "apps/customers/src",
      "compilerOptions": {
        "tsConfigPath": "apps/customers/tsconfig.app.json"
      }
    },
    "orders": {
      "type": "application",
      "root": "apps/orders",
      "entryFile": "main",
      "sourceRoot": "apps/orders/src",
      "compilerOptions": {
        "tsConfigPath": "apps/orders/tsconfig.app.json"
      }
    },
    "meals": {
      "type": "application",
      "root": "apps/meals",
      "entryFile": "main",
      "sourceRoot": "apps/meals/src",
      "compilerOptions": {
        "tsConfigPath": "apps/meals/tsconfig.app.json"
      }
    },
    "discounts": {
      "type": "application",
      "root": "apps/discounts",
      "entryFile": "main",
      "sourceRoot": "apps/discounts/src",
      "compilerOptions": {
        "tsConfigPath": "apps/discounts/tsconfig.app.json"
      }
    },
    "menu": {
      "type": "application",
      "root": "apps/menu",
      "entryFile": "main",
      "sourceRoot": "apps/menu/src",
      "compilerOptions": {
        "tsConfigPath": "apps/menu/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    }
  }
}